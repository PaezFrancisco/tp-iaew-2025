# Variables de entorno para desarrollo

# Puerto del servidor
PORT=3000

# Base de datos PostgreSQL
DATABASE_URL="postgresql://postgres:password@localhost:5432/reserva_turnos_db?schema=public"

# Configuración de la aplicación
NODE_ENV=development
API_VERSION=v1

# Configuración de seguridad (para futuro uso con Keycloak)
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=24h

# Configuración de RabbitMQ (para futuro uso)
RABBITMQ_URL=amqp://localhost:5672

# Configuración de logs (para futuro uso con Kibana)
LOG_LEVEL=info
LOG_FORMAT=json
