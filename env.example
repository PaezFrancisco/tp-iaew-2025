# ============================================
# VARIABLES DE ENTORNO
# ============================================

# ============================================
# SERVIDOR
# ============================================
PORT=3000
NODE_ENV=development

# ============================================
# BASE DE DATOS
# ============================================
DATABASE_URL=postgresql://health_app_user:health_app_password@localhost:5432/health_app_db?schema=public

# ============================================
# RABBITMQ
# ============================================
RABBITMQ_URL=amqp://health_app_user:health_app_password@localhost:5672

# ============================================
# KEYCLOAK - OAuth2 Provider
# ============================================
KEYCLOAK_URL=http://localhost:8080
KEYCLOAK_REALM=health_app
KEYCLOAK_CLIENT_ID=health_app_api
KEYCLOAK_CLIENT_SECRET=

# ============================================
# ELASTICSEARCH
# ============================================
ELASTICSEARCH_URL=http://localhost:9200

# ============================================
# WEBHOOK
# ============================================
WEBHOOK_URL=http://httpbin.org/post

# ============================================
# LOGS
# ============================================
LOG_LEVEL=info

